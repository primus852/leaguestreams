{
    "type": "project",
    "name": "primus852/leaguestreams",
    "description": "Crawl Streamers League Of Legends' Games",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "primus852/simple-crypt": "^1.2",
        "primus852/simple-stopwatch": "^1.0",
        "primus852/symfony-short-response": "^1.2",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "^5.0",
        "symfony/browser-kit": "^5.0",
        "symfony/cache": "^5.0",
        "symfony/config": "^5.0",
        "symfony/console": "^5.0",
        "symfony/css-selector": "^5.0",
        "symfony/debug-pack": "^1.0",
        "symfony/dotenv": "^5.0",
        "symfony/expression-language": "^5.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/maker-bundle": "^1.9",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "^5.0",
        "symfony/security-bundle": "^5.0",
        "symfony/serializer-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^5.0",
        "symfony/validator": "^5.0",
        "symfony/web-link": "^5.0",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "^5.0",
        "twig/extensions": "^1.5"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/web-server-bundle": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C83M2RDTGBCX7W7SJ463XVAT",
            "allow-contrib": false,
            "require": "^5.0"
        }
    }
}
